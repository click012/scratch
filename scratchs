=LET(
    txt, A1,
    chars, "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ",
    arr, MID(txt, ROW(INDIRECT("1:" & LEN(txt))), 1),
    sum, SUMPRODUCT(SEARCH(arr, chars) - 1),
    MID(chars, MOD(sum, 36) + 1, 1)
)


=MID("0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ", MOD(SUMPRODUCT(SEARCH(MID(A1,ROW(INDIRECT("1:"&LEN(A1))),1),"0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ")-1),36)+1, 1)


